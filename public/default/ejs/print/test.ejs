<style>
    .print .line {
        height: 3px;
        background-color: #000;
        position: relative;
    }
    .print .line div {
        height: 23px;
        width: 3px;
        background-color: #000;
        position: absolute;
        top: -10px;
    }
    .print .line .start {
        left: 0;
    }
    .print .line .center {
        left: 50%;
    }
    .print .line .end {
        right: 0;
    }
</style>

<div class="print" style="width: 560px;">
    <div class="py-5 text-center" style="font-size: 30px;">
        <p class="mb-3"><img width="400" height="64"
                src="data:image/png;base64,">
        </p>
        <p class="mb-3">Test print</p>
        <p class="mb-5"><%= moment().tz('Asia/Tokyo').locale('ja').format('YYYY/MM/DD HH:mm:ss') %></p>
        <div class="line">
            <div class="start"></div>
            <div class="center"></div>
            <div class="end"></div>
        </div>
    </div>
</div>